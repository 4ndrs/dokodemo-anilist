{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",

    "moduleResolution": "node",
    "noEmit": true,

    "jsx": "react-jsx",

    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
